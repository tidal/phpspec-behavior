{
  "name": "tidal/phpspec-behavior",
  "description": "An extension for PhpSpec to generate behavior (traits) from interfaces",
  "keywords": ["phpspec", "extension", "code-generation", "code-generator", "behavior", "trait", "interface", "implementation", "test", "bdd", "php7"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Timo Michna",
      "email": "timomichna@yahoo.de"
    }
  ],
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "phpspec/phpspec": "^3.0",
    "tidal/phpspec-console": "^1|^0.2.0"
  },
  "require-dev": {
    "behat/behat": "^3.0,>=3.0.4",
    "rmiller/caser": "^0.1",
    "memio/spec-gen": "^0.7",
    "rmiller/behat-spec": "^0.5.0",
    "hamcrest/hamcrest-php": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Tidal\\PhpSpec\\BehaviorExtension\\": "src",
      "spec\\Tidal\\PhpSpec\\BehaviorExtension\\": "spec",
      "features\\": "features"
    }
  }
}
